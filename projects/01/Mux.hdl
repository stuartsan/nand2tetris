/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=sel, b=b, out=and1);
    And(a=notsel, b=a, out=and2);
    Or(a=and1, b=and2, out=out);
}